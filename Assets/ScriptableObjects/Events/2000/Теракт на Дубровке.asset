    \u0442\u0440\u0438 \u043C\u0438\u043A\u0440\u043E\u0430\u0432\u0442\u043E\u0431\u0443\u0441\u0430
    \u0441 \u0432\u043E\u043E\u0440\u0443\u0436\u0451\u043D\u043D\u044B\u043C\u0438
    \u0431\u043E\u0435\u0432\u0438\u043A\u0430\u043C\u0438, \u043A\u043E\u0442\u043E\u0440\u044B\u0435
    \u0437\u0430\u0431\u0435\u0436\u0430\u043B\u0438 \u0432 \u0437\u0434\u0430\u043D\u0438\u0435.
    \u041E\u0441\u043D\u043E\u0432\u043D\u0430\u044F \u0433\u0440\u0443\u043F\u043F\u0430
    \u0410\u0431\u0443\u0431\u0430\u043A\u0430\u0440\u0430 \u0437\u0430\u0445\u0432\u0430\u0442\u0438\u043B\u0430
    \u043A\u043E\u043D\u0446\u0435\u0440\u0442\u043D\u044B\u0439 \u0437\u0430\u043B
    \u0438 \u0432\u0437\u044F\u043B\u0430 \u0432 \u0437\u0430\u043B\u043E\u0436\u043D\u0438\u043A\u0438
    916 \u0447\u0435\u043B\u043E\u0432\u0435\u043A.\n\n\u041F\u043E \u043C\u043D\u0435\u043D\u0438\u044E
    \u0440\u043E\u0441\u0441\u0438\u0439\u0441\u043A\u0438\u0445 \u0432\u043B\u0430\u0441\u0442\u0435\u0439,
    \u0446\u0435\u043B\u044C\u044E \u0442\u0435\u0440\u0440\u043E\u0440\u0438\u0441\u0442\u0438\u0447\u0435\u0441\u043A\u043E\u0439
    \u0430\u043A\u0446\u0438\u0438 \u0431\u044B\u043B\u043E \u043D\u0430\u0440\u0443\u0448\u0435\u043D\u0438\u0435
    \u043E\u0431\u0449\u0435\u0441\u0442\u0432\u0435\u043D\u043D\u043E\u0439 \u0431\u0435\u0437\u043E\u043F\u0430\u0441\u043D\u043E\u0441\u0442\u0438,
    \u0443\u0441\u0442\u0440\u0430\u0448\u0435\u043D\u0438\u0435 \u043D\u0430\u0441\u0435\u043B\u0435\u043D\u0438\u044F
    \u0438 \u043E\u043A\u0430\u0437\u0430\u043D\u0438\u0435 \u0432\u043E\u0437\u0434\u0435\u0439\u0441\u0442\u0432\u0438\u044F
    \u043D\u0430 \u043E\u0440\u0433\u0430\u043D\u044B \u0432\u043B\u0430\u0441\u0442\u0438
    \u0420\u043E\u0441\u0441\u0438\u0439\u0441\u043A\u043E\u0439 \u0424\u0435\u0434\u0435\u0440\u0430\u0446\u0438\u0438
    \u043F\u043E \u043F\u0440\u0438\u043D\u044F\u0442\u0438\u044E \u0440\u0435\u0448\u0435\u043D\u0438\u044F
