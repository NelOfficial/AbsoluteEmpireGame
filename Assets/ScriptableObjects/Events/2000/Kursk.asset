    \u0412 \u0440\u0435\u0437\u0443\u043B\u044C\u0442\u0430\u0442\u0435 \u043D\u0430\u0447\u0430\u0432\u0448\u0435\u0439\u0441\u044F
    \u0435\u0449\u0451 \u043D\u0430 \u0431\u0435\u0440\u0435\u0433\u0443 \u043F\u043E\u0441\u043B\u0435\u0434\u043E\u0432\u0430\u0442\u0435\u043B\u044C\u043D\u043E\u0441\u0442\u0438
    \u043D\u0430\u0440\u0443\u0448\u0435\u043D\u0438\u0439 \u0432 \u043F\u0440\u043E\u0446\u0435\u0434\u0443\u0440\u0430\u0445
    \u043E\u0431\u0440\u0430\u0449\u0435\u043D\u0438\u044F \u0441 \u0442\u043E\u0440\u043F\u0435\u0434\u043E\u0439
    65-76\u0410 \u0432 \u043F\u0435\u0440\u0432\u043E\u043C \u043E\u0442\u0441\u0435\u043A\u0435
    \u043F\u0440\u043E\u0438\u0437\u043E\u0448\u0451\u043B \u0432\u0437\u0440\u044B\u0432
    \u043A\u0438\u0441\u043B\u043E\u0440\u043E\u0434\u043D\u043E-\u043A\u0435\u0440\u043E\u0441\u0438\u043D\u043E\u0432\u043E\u0439
    \u0441\u043C\u0435\u0441\u0438 \u0438 \u0432\u043E\u0437\u043D\u0438\u043A \u043F\u043E\u0436\u0430\u0440,
    \u0447\u0442\u043E \u043F\u0440\u0438\u0432\u0435\u043B\u043E \u043A \u0432\u044B\u0445\u043E\u0434\u0443
    \u0438\u0437 \u0441\u0442\u0440\u043E\u044F \u044D\u043A\u0438\u043F\u0430\u0436\u0430
    \u043F\u0435\u0440\u0432\u044B\u0445 \u0442\u0440\u0451\u0445 \u043E\u0442\u0441\u0435\u043A\u043E\u0432,
    \u0432\u043A\u043B\u044E\u0447\u0430\u044F \u0433\u043B\u0430\u0432\u043D\u044B\u0439
    \u043A\u043E\u043C\u0430\u043D\u0434\u043D\u044B\u0439 \u043F\u0443\u043D\u043A\u0442.
    \u041D\u0435\u0443\u043F\u0440\u0430\u0432\u043B\u044F\u0435\u043C\u0430\u044F
    \u043B\u043E\u0434\u043A\u0430 \u043F\u043E\u0448\u043B\u0430 \u043A\u043E \u0434\u043D\u0443,
    \u0438 \u0443\u0434\u0430\u0440 \u043E\u0431 \u043D\u0435\u0433\u043E \u043F\u0440\u0438\u0432\u0451\u043B
    \u043A\u043E \u0432\u0437\u0440\u044B\u0432\u0443 \u0442\u043E\u0440\u043F\u0435\u0434\u043D\u043E\u0433\u043E
    \u0431\u043E\u0435\u0437\u0430\u043F\u0430\u0441\u0430 \u043B\u043E\u0434\u043A\u0438.
