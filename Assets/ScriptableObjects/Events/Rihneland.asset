    \u043A\u0430\u043A \u0437\u0430\u0449\u0438\u0442\u043D\u0443\u044E \u0438\u043D\u0438\u0446\u0438\u0430\u0442\u0438\u0432\u0443
    \u043F\u0440\u043E\u0442\u0438\u0432 \xAB\u043E\u043A\u0440\u0443\u0436\u0435\u043D\u0438\u044F\xBB
