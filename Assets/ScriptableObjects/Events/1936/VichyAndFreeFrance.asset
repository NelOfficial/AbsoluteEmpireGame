    \u0441\u0442\u0435\u043F\u0435\u043D\u0438 \u0438\u0437-\u0437\u0430 \u043F\u043E\u043F\u0443\u0441\u0442\u0438\u0442\u0435\u043B\u044C\u0441\u0442\u0432\u0430
    \u043D\u0430\u0446\u0438\u0441\u0442\u0430\u043C \u0432 \u0440\u0430\u043C\u043A\u0430\u0445
    \u0442\u0430\u043A \u043D\u0430\u0437\u044B\u0432\u0430\u0435\u043C\u043E\u0439
