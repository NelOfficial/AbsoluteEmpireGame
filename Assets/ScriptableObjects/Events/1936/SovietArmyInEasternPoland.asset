    \u043F\u043E\u0445\u043E\u0434 \u0420\u041A\u041A\u0410, \u0432 \u043F\u043E\u043B\u044C\u0441\u043A\u043E\u0439
    \u0438\u0441\u0442\u043E\u0440\u0438\u043E\u0433\u0440\u0430\u0444\u0438\u0438
    \u0432\u0442\u043E\u0440\u0436\u0435\u043D\u0438\u0435 \u0421\u0421\u0421\u0420
    \u0432 \u041F\u043E\u043B\u044C\u0448\u0443) \u2014 \u0432\u043E\u0435\u043D\u043D\u0430\u044F
    \u043E\u043F\u0435\u0440\u0430\u0446\u0438\u044F \u0420\u0430\u0431\u043E\u0447\u0435-\u043A\u0440\u0435\u0441\u0442\u044C\u044F\u043D\u0441\u043A\u043E\u0439
    \u041A\u0440\u0430\u0441\u043D\u043E\u0439 \u0430\u0440\u043C\u0438\u0438 \u0432
    \u0432\u043E\u0441\u0442\u043E\u0447\u043D\u044B\u0445 \u043E\u0431\u043B\u0430\u0441\u0442\u044F\u0445
    \u041F\u043E\u043B\u044C\u0441\u043A\u043E\u0439 \u0420\u0435\u0441\u043F\u0443\u0431\u043B\u0438\u043A\u0438,
    \u043F\u0440\u043E\u0432\u043E\u0434\u0438\u0432\u0448\u0430\u044F\u0441\u044F
    \u0441 17 \u0441\u0435\u043D\u0442\u044F\u0431\u0440\u044F \u043F\u043E 5 \u043E\u043A\u0442\u044F\u0431\u0440\u044F
    1939 \u0433\u043E\u0434\u0430 \u0438 \u043F\u043E\u0432\u043B\u0451\u043A\u0448\u0430\u044F
    \u043F\u0440\u0438\u0441\u043E\u0435\u0434\u0438\u043D\u0435\u043D\u0438\u0435
    \u044D\u0442\u0438\u0445 \u0442\u0435\u0440\u0440\u0438\u0442\u043E\u0440\u0438\u0439
    \u043A \u0423\u043A\u0440\u0430\u0438\u043D\u0441\u043A\u043E\u0439 \u0438 \u0411\u0435\u043B\u043E\u0440\u0443\u0441\u0441\u043A\u043E\u0439
    \u0421\u0421\u0420 (\u0432 \u043A\u0430\u0447\u0435\u0441\u0442\u0432\u0435 \u0417\u0430\u043F\u0430\u0434\u043D\u043E\u0439
    \u0423\u043A\u0440\u0430\u0438\u043D\u044B \u0438 \u0417\u0430\u043F\u0430\u0434\u043D\u043E\u0439
