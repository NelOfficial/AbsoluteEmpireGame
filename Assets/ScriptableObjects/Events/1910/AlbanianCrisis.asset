    \u0414\u0443\u0440\u0440\u0435\u0441\u043E\u043C, \u043D\u0430 \u043A\u043E\u0442\u043E\u0440\u043E\u0439
    \u0431\u044B\u043B \u043E\u0431\u0440\u0430\u0437\u043E\u0432\u0430\u043D \u043E\u043A\u0440\u0443\u0433
    \u0414\u0440\u0430\u0447. \u041E\u0434\u043D\u0430\u043A\u043E \u0441\u0435\u0440\u0431\u0430\u043C
    \u043D\u0435 \u0443\u0434\u0430\u043B\u043E\u0441\u044C \u043D\u0430 \u0434\u043E\u043B\u0433\u043E
    \u0443\u0434\u0435\u0440\u0436\u0430\u0442\u044C\u0441\u044F \u043D\u0430 \u044D\u0442\u043E\u0439
    \u0442\u0435\u0440\u0440\u0438\u0442\u043E\u0440\u0438\u0438 \u0438 \u0443\u0436\u0435
    \u0432 \u0430\u043F\u0440\u0435\u043B\u0435 1913 \u0433\u043E\u0434\u0430 \u043E\u043D\u0438
    \u043F\u043E\u043A\u0438\u043D\u0443\u043B\u0438 \u0410\u0434\u0440\u0438\u0430\u0442\u0438\u0447\u0435\u0441\u043A\u043E\u0435
